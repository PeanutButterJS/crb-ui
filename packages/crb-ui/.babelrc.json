{
  "presets": [
    ["@babel/env", { "modules": false }],
    "@babel/preset-typescript",
    ["@babel/preset-react", { "runtime": "automatic" }]
  ],
  "plugins": [
    ["@babel/plugin-transform-runtime", { "useESModules": true }],
    [
      "babel-plugin-styled-components",
      {
        "pure": true,
        "namespace": "crb-ui"
      }
    ]
  ]
}
